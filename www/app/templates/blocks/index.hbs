{{#if model.matured}}
<h4>Matured Transactions</h4>
<div class="table-responsive">
<table class="table table-condensed table-striped">
  <thead>
    <tr>
      <th>Tx Hash</th>
      <th>Login</th>
      <th>Time Found</th>
      <th>Variance</th>
      <th>Reward</th>
      <th>Type</th>
      <th>Shares/Diff</th>
      <th>Worker ID</th>
    </tr>
  </thead>
  <tbody>
    {{#each model.matured as |block|}}
      {{partial "blocks/block"}}
    {{/each}}
  </tbody>
</table>
</div>
{{else}}
<h3>No matured transactions yet</h3>
{{/if}}
